<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.example.tanggalwaktu2">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".AppController"
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppThemeNoActionBar">
        <activity android:name=".tanyajawab"></activity>
        <activity android:name=".petajarak" />
        <activity android:name=".tatacarahaji" />
        <activity android:name=".tatacaraumroh" />
        <activity android:name=".fiqihhaji" />
        <activity android:name=".talbiyah" />
        <activity android:name=".teman" />
        <activity android:name=".harta_halal" />
        <activity android:name=".taubat" />
        <activity android:name=".jgnmaksiat" />
        <activity android:name=".berdebat" />
        <activity android:name=".meneladani_nabi" />
        <activity android:name=".ikhlas" />
        <activity android:name=".bersabar" />
        <activity android:name=".segeralah" />
        <activity android:name=".jamaah" />
        <activity android:name=".keutamaan" />
        <activity android:name=".lokasiZiarah"/>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".Navigation"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".searchview"
            android:parentActivityName=".Navigation"
            android:screenOrientation="portrait" />
        <activity
            android:name=".DaftarIsi"
            android:parentActivityName=".Navigation"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Tentang"
            android:parentActivityName=".Navigation"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Rate"
            android:parentActivityName=".Navigation"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Maps"
            android:parentActivityName=".Navigation"
            android:screenOrientation="portrait" />
        <activity
            android:name=".persiapanhaji2"
            android:parentActivityName=".Navigation"
            android:screenOrientation="portrait" />
        <activity
            android:name=".FiqihUmrah"
            android:parentActivityName=".Navigation"
            android:screenOrientation="portrait" />
        <activity
            android:name=".DzikirDoa"
            android:parentActivityName=".Navigation"
            android:screenOrientation="portrait" />
    </application>

</manifest>